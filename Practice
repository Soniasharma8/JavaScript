  console.log("s");
  console.log("o");
  console.log("n");
  console.log("i");
  console.log("a");
}

// sayName();

function addTwoNumbers(number1, number2) {
  console.log(number1 + number2);
}
// addTwoNumbers(3, 10);

function loginUserMessage(userName) {
  if (userName) {
    console.log(`${userName} just logged in`);
  } else {
    console.log("please enter a username");
  }
}
// loginUserMessage();



