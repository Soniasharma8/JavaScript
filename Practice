// objects

//Object literals

const mySym = Symbol("key1");

const JsUser = {
  name: "sonia",
  [mySym]: "myKey1",
  age: 22,
  email: "sonia@gmail",
  location: "india",
  isLoggedIn: false,
  lastLoginDays: ["monday", "tuesday", "wednesday", "thursday", "friday"],
};

// console.log(JsUser.email);
// console.log(JsUser.name);
// console.log(JsUser[mySym]);

